} else if (command === 'info') {
        var info = new Discord.RichEmbed()
        .setTitle('Bot Commands')
        .addField('Owner', 'mystermegz')
        .addField('Prefix', '$')
        .addField('Commands', 'hug,slap,facedesk,ping,hug,eat,bed,pat')
        .setColor(0x3958D0);
        msg.channel.sendEmbed(info);
